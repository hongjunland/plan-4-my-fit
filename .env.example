# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# OpenAI Configuration
VITE_OPENAI_API_KEY=your_openai_api_key

# App Configuration
VITE_APP_URL=http://localhost:5173
VITE_APP_NAME="Plan4MyFit"
VITE_APP_VERSION=1.0.0

# Monitoring & Analytics
VITE_SENTRY_DSN=your_sentry_dsn_url
SENTRY_ORG=your_sentry_org
SENTRY_PROJECT=your_sentry_project
SENTRY_AUTH_TOKEN=your_sentry_auth_token

# PWA Configuration
VITE_ENABLE_PWA=true

# Development
NODE_ENV=development

# Google Calendar API Configuration
# Get these from Google Cloud Console > APIs & Services > Credentials
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
VITE_GOOGLE_CALENDAR_REDIRECT_URI=http://localhost:5173/auth/google-calendar/callback

# Note: Google OAuth for login is handled through Supabase
# Note: Google Calendar API requires separate OAuth credentials
# Note: GOOGLE_CLIENT_SECRET should be stored in Supabase Secrets (not in client env)
# Note: Vercel Analytics is automatically enabled on Vercel deployment