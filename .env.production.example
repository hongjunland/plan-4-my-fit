# Production Environment Variables Template
# Copy this file to .env.production.local for production deployment

# Supabase Configuration (Production)
VITE_SUPABASE_URL=https://your-production-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-production-anon-key

# OpenAI Configuration (Production)
VITE_OPENAI_API_KEY=your-production-openai-api-key

# App Configuration (Production)
VITE_APP_URL=https://your-domain.com
VITE_APP_NAME="Plan4MyFit"
VITE_APP_VERSION=1.0.0

# Environment
NODE_ENV=production

# Analytics & Monitoring (Optional)
VITE_SENTRY_DSN=your-sentry-dsn
VITE_GA_TRACKING_ID=your-google-analytics-id

# Feature Flags (Optional)
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PERFORMANCE_MONITORING=true